{
	"sun": {
		"label":"sun",
		"url": "http://sun.mv/feed/",
		"scrape":{
			"headlines":{
				"selector":["item title"]
			},
			"links":{
				"selector":["item url"]
			},
			"description":{
				"selector":["item description"]
			},
			"image":{
				"selector":["item image"]
			}
		}
	},
	"vnews": {
		"label":"vnews",
